{
    "name": "sorry510/constant",
    "description": "laravel constant",
    "keywords": ["laravel", "const"],
    "license": "MIT",
    "authors": [
        {
            "name": "sorry510",
            "email": "491559675@qq.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sorry510\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sorry510\\ConstantsProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
